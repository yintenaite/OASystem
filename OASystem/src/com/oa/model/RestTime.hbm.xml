<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.oa.model">

	<class name="RestTime" table="RestTime">
		<id name="rtId"><!-- 主键 -->
			<!-- 
				主键生成策略
				1.native:是根据数据库类型，决定主键的自动生成策略		
				   比如mysql使用auto_increment自增长
				2.increment:从1开始增长，每次+1，适用于所有数据库
				3.uuid:生成网络间唯一标识的方式，经常在商品化的软件使用
				       生成uuid会占用比较多的空间
			 -->
			<generator class="increment"/>
		</id>
		<property name="onTime"/>
		<property name="offTime"/>
		<property name="rtStartTime"/>
		<property name="rtFinalTime"/>
	</class>
</hibernate-mapping>
